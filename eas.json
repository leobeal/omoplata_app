{
  "cli": {
    "version": ">= 10.0.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "TENANT": "main"
      }
    },
    "ios-simulator": {
      "ios": {
        "simulator": true
      },
      "env": {
        "TENANT": "main"
      },
      "channel": "ios-simulator"
    },
    "android-simulator": {
      "env": {
        "TENANT": "main"
      },
      "channel": "android-simulator"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "TENANT": "main"
      }
    },
    "main": {
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production",
        "TENANT": "main"
      },
      "channel": "production"
    },
    "evolve-grappling": {
      "autoIncrement": true,
      "env": {
        "TENANT": "evolve-grappling"
      },
      "channel": "evolve-grappling"
    },
    "sparta-aachen": {
      "autoIncrement": true,
      "env": {
        "TENANT": "sparta-aachen"
      },
      "channel": "sparta-aachen"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "1234567890",
        "appleTeamId": "ABCDE12345"
      },
      "android": {
        "serviceAccountKeyPath": "./secrets/google-service-account.json",
        "track": "production"
      }
    },
    "internal": {
      "android": {
        "serviceAccountKeyPath": "./secrets/google-service-account.json",
        "track": "internal",
        "releaseStatus": "draft"
      }
    },
    "alpha": {
      "android": {
        "serviceAccountKeyPath": "./secrets/google-service-account.json",
        "track": "alpha"
      }
    },
    "release": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "1234567890",
        "appleTeamId": "ABCDE12345"
      },
      "android": {
        "serviceAccountKeyPath": "./secrets/google-service-account.json",
        "track": "production"
      }
    }
  }
}
